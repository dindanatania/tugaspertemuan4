package tugas4.no2;

import java.util.Scanner;

public class Tugas4No2 {

    public static void main(String[] args) {
        
        Scanner input = new Scanner(System.in);
        
        System.out.println("Masukkan nilai batas awal: ");
        int batasAwal = input.nextInt();
        
        System.out.println("Masukkan nilai batas akhir: ");
        int batasAkhir = input.nextInt();
        
        int i;
        for(i = batasAwal; i <= batasAkhir; i++){
            if(i%2 != 0){
                System.out.println(i);
            }
        }
    }
    
}
